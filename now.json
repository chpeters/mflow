{
  "version": 2,
  "builds": [
    { "src": "frontend/package.json", "use": "@now/static-build" },
    { "src": "api/!(lib)/**/*.js", "use": "@now/node" }
  ],
  "routes": [
    { "src": "^/static/(.*)", "dest": "/frontend/static/$1" },
    { "src": "/api/(.*)", "dest": "/api/$1.js" },
    { "src": ".*", "dest": "/frontend/index.html" }
  ],
  "alias": ["www.mflow.tech", "mflow.tech"]
}
